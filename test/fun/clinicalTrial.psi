def main(){
    controlGroup := [0,0,1,0,0];
    treatedGroup := [1,0,1,1,1];

    isEffective := flip(1/2);
    probIfTreated := beta(1,1);
    probIfControl := if isEffective { beta(1,1) } else { probIfTreated };

    for i in [0..controlGroup.length){
        observe(controlGroup[i] == flip(probIfControl));
    }
    for i in [0..treatedGroup.length){
        observe(treatedGroup[i] == flip(probIfTreated));
    }

    isEffectiveP := sample(Marginal(isEffective));
    observe(isEffective);
    return (isEffectiveP,probIfTreated,probIfControl); // expected: (-3600·probIfControl²·probIfTreated⁴+-3600·probIfControl⁴·probIfTreated⁴+-5400·probIfControl³·probIfTreated⁵+-900·probIfControl·probIfTreated⁵+-900·probIfControl⁵·probIfTreated⁵+3600·probIfControl²·probIfTreated⁵+3600·probIfControl⁴·probIfTreated⁵+5400·probIfControl³·probIfTreated⁴+900·probIfControl·probIfTreated⁴+900·probIfControl⁵·probIfTreated⁴)·(25/102·δ(0)[isEffectiveP]+77/102·δ(1)[isEffectiveP])·[-1+probIfControl≤0]·[-1+probIfTreated≤0]·[-probIfControl≤0]·[-probIfTreated≤0]
}
