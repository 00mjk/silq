def main(){
    x := Bernoulli(1/2);
    if x {
        assert(x);
	observe(x);
    }else{
        assert(!x);
	observe(!x);
    }

    y := if x { 1/x } else { 1/(1-x) };
    z := 0;
    if x { z = 1/x; } else { z=1/(1-x); }

    w := if x { Bernoulli(1/2) } else { Bernoulli(1/3) };
    return (y,z,w);
}