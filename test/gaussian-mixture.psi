
def main(){
    tau := 1;
    sigma := 1;
    mu1 := gauss(0,tau^2);
    mu2 := gauss(0,tau^2);
    
    x1 := if flip(1/3) { gauss(mu1, sigma) } else { gauss(mu2, sigma) };
    x2 := if flip(1/3) { gauss(mu1, sigma) } else { gauss(mu2, sigma) };
    return (x1,x2); // expected: 5/18·e^(-x1²·⅟3+-x2²·⅟3+x1·x2·⅟3)·⅟π·⅟√3̅+e^(-x1²·⅟4+-x2²·⅟4)·⅟9·⅟π
}
