
def main(){
	x := flip(1/2);
	y := flip(1/3);
	dat Foo{
		dat Bar{
			a : ℝ;
			b : ℝ;
			def Bar(z: ℝ){
				a = x;
				b = z;
			}
		}
		b: Bar;
		def Foo(){
			b = Bar(y);
		}
		def foo(){
			return b.b;
		}
		def bar(){
			return b.a;
		}
	}
	k := Foo().bar();
	l := Foo().foo();
	return (k,l); // expected: (2/3·δ[l]+δ[-l+1]·⅟3)·(δ[-k+1]·⅟2+δ[k]·⅟2)
}
