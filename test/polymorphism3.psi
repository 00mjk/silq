
arrayLength := 3; // TODO: make unnecessary

def map[a,b](lambda: a → b){
    return (arr: a[]){
        result := ([]:b[]);
        for i in [0..arrayLength){
            result ~= [lambda(arr[i])];
        }
        return result;
    }
}

def cat2[a](x:a[]){
    return x~x;
}

def main(){
    r:=map((x)=>x+1)([1,2,3]);
    t:=map((x:ℝ×ℝ[])=>x[1]~[x[0]])([(0,r),(1,r),(2,r)]);
    s:=cat2([1,2]);
    return (r[0],r[1],r[2],t[0][3],t[1][3],s[0],s[1],s[2],s[3]); // expected: δ(0)[r₄]·δ(1)[r₅]·δ(1)[r₆]·δ(1)[r₈]·δ(2)[r₁]·δ(2)[r₇]·δ(2)[r₉]·δ(3)[r₂]·δ(4)[r₃]
}
