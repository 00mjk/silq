def main(){ // bothHeads
    firstCoin := flip(1/2);
    secondCoin := flip(1/2);
    bothHeads := firstCoin && secondCoin;
    observe(bothHeads == 0);
    firstCoin = Expectation(firstCoin);
    secondCoin = Expectation(secondCoin);
    return (firstCoin,secondCoin); // expected: δ(0)[-firstCoin+1/3]·δ(0)[-secondCoin+1/3]
}
