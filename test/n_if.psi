def main(){
    x := uniformInt(1,10);
    i := 1;
    y := 0;
    repeat 5{
        if flip(1/2) && x == i {
            y = y + 1;
        }
        i = i + flip(1/2);
    }
    return y; // expected: 1/5120·δ(0)[-y+5]+19/512·δ(0)[-y+2]+23/2560·δ(0)[-y+3]+4153/5120·δ(0)[y]+721/5120·δ(0)[-y+1]+9/5120·δ(0)[-y+4]
}
