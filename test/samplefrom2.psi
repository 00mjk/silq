
def newDistr(w){
    (x,y,z) := sampleFrom("(x,y,z;w) => [-1<=x]*[x<=1]*[-1<=y]*[y<=1]*|x|*|y|*delta(0)[z-w]",w);
    return (x,y,z);
}

