
def main(){
    r := false;
    r ||= flip(1/2);
    s := true;
    s &&= flip(1/2);
    t := true;
    t ||= flip(1/2);
    u := false;
    u &&= flip(1/2);

    v := flip(1/2) && flip(1/2);
    w := flip(1/2) || flip(1/2);

    x := r&&r;
    y := s||s;

    return (r,s,t,u,v,w,x,y); // expected: (1/2·δ(0)[r]+1/2·δ(1)[r])·(1/2·δ(0)[s]+1/2·δ(1)[s])·(1/4·δ(0)[w]+3/4·δ(1)[w])·(1/4·δ(1)[v]+3/4·δ(0)[v])·([r=0]·δ(0)[x]+[r≠0]·δ(1)[x])·([s=0]·δ(0)[y]+[s≠0]·δ(1)[y])·δ(0)[u]·δ(1)[t]
}
